                                <div class="pt1">
                                    缴清全额</div>
                                <div class="pt">
                                    汇款</div>
                                <div class="pt">
                                    2011/03/30</div>
                                <div class="pt">
                                    0</div>
                                <div class="pt">
                                    普通发票</div>
                                <div class="pt">
                                    231001070052</div>
                                <div class="pt">
                                </div>
                                <div class="clear">
                                </div>
                            </div>
                            <!--show-->
                        </div>
                    </div>
                    <!--力史start-->
                    <div align="right">
                        已付金额(人民币)<span id="pay1">0</span></div>
                    <div align="right">
                        未付金额(人民币)<span id="pay2">0</span></div>
                    </form>*@
                </div>
            </div>
            <!--內容end-->
            <font id="div_abc_inner_main_contents"></font>
        </div>
    </div>
</div>
</div>